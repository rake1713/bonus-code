#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    if (N < 1) {
        cout << "NO" << endl;
        return 0;
    }

    int d = 1;
    while (d < N) {
        d *= 2;
    }

    if (d == N) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }

    return 0;
}
