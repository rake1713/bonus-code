#include <iostream>
using namespace std;

int main() {
    int x, y;
    cin >> x >> y;
    if (x == 1) {
        if (y == 1) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    } else {
        if (y != 1) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    return 0;
}
